include		GENERIC

ident		VIMAGE

#
# Virtual networking for jails
#
options		VIMAGE

#
# Firewall default to accept everything
#
options		IPFIREWALL
options		IPFIREWALL_VERBOSE
options		IPFIREWALL_VERBOSE_LIMIT=100
options		IPFIREWALL_DEFAULT_TO_ACCEPT

#
# Don't build kernel with debug symbols
#
nomakeoptions	DEBUG
